<p> Okleina jednostronna</p>\n        </label>   \n    </div>\n    <div>\n        <label class=\"shadow p-3 mb-5 bg-white rounded\">\n            <input type=\"radio\" id=\"type2\" value=\"dwustronna\" v-model=\"winVenner\">\n            <img src='/images/dwustronna.jpg' alt=\"window glass\" width=\"256\" height=\"256\">\n            <p> Okleina dwustronna</p>\n        </label>\n    </div>\n</div>\n</div>\n    <div>{{ this.$store.state.winSizePrice }}</div>\n    <div>{{ this.$store.state.winArea }}</div>\n    <div> {{ winVenner }} </div>\n    <div>\n        <button @click=\"navigateToWinSize\" type=\"button\" class=\"btn btn-danger previous\">Poprzedni</button>\n        <button @click=\"setWinVenner\" type=\"button\" class=\"btn btn-danger next\">Następny</button>\n    </div>   \n</template>\n\n<script>\n    export default {\n        data() {\n            return {\n                winVenner: \"\",\n                inputIsInvalid: false\n            };\n        },\n        methods: {\n            setWinVenner() {\n                const windowVenner = this.winVenner;\n                const winSizePrice = this.$store.state.winSizePrice;\n                if(windowVenner == '') {\n                    this.inputIsInvalid = true;\n                    return;\n                } else if(windowVenner == 'jednostronna') {\n                    const winVennerPrice = Math.round((winSizePrice * 1.25) * 100) / 100;\n                    this.$store.commit('setWinVennerPrice', winVennerPrice);\n                } else if (windowVenner == 'dwustronna') {\n                    const winVennerPrice = Math.round((winSizePrice * 1.44) * 100) / 100;\n                    this.$store.commit('setWinVennerPrice', winVennerPrice);\n                } else {\n                    return 0;\n                }\n                this.$store.commit('setWinVenner', windowVenner);\n                this.$router.push('/windowGlass');\n            },\n            navigateToWinSize() {\n                this.$router.push(\"/windowSize\");\n            },\n            confrimError() {\n                this.inputIsInvalid = false;\n            },\n        }\n    }\n\n</script>\n\n<style scoped>\n    .choose {\n     display: flex;\n     flex-direction: row;\n     flex-flow: row wrap;\n     justify-content: center;\n }\n\n  [type=radio] { \n  position: absolute;\n  opacity: 0;\n  width: 0;\n  height: 0;\n}\n\n/* IMAGE STYLES */\n[type=radio] + img {\n  cursor: pointer;\n}\n\n/* CHECKED STYLES */\n[type=radio]:checked + img {\n  outline: 2px solid black;\n}\n.previous {\n    float: left;\n    padding: 10px 50px;\n    display: inline-block;\n    margin-left: 20px;\n}\n.header {\n    font-size: 20px;\n    padding: 5px;\n    text-align: center;\n    background-color: #D7DBDD;\n    margin: 0;\n    padding: 0;\n}\n</style>"]}]}